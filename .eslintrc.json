{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "extends": [
    "@rocketseat/eslint-config/next",
    "next/core-web-vitals"
  ],
  "plugins": [
    "simple-import-sort",
    "react"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "import/newline-after-import": "error",
    "react/jsx-key": "error"
  }
}